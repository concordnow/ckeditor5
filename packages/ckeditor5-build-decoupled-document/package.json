    "type": "git",
    "url": "https://github.com/ckeditor/ckeditor5-build-decoupled-document.git"
  },
  "scripts": {
    "build": "yarn run removeModules && yarn install && webpack --mode production",
    "dev": "webpack --mode development",
    "lint": "eslint --quiet '**/*.js'",
    "preversion": "yarn run build; if [ -n \"$(git status src/ckeditor.js build/ --porcelain)\" ]; then git add -u src/ckeditor.js build/ && git commit -m 'Internal: Build.'; fi",
    "test": "ckeditor5-dev-tests",
    "manual": "ckeditor5-dev-tests-manual",
    "removeModules": "rm -rf ./packages ./node_modules"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ]
  },
  "eslintIgnore": [
    "build/**",
    "packages/**"
  ],
  "workspaces": {
    "packages": [
      "packages/*",
      "."
    ],
    "nohoist": [
      "**/husky"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
